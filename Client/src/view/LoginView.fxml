<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginViewController">
   <children>
      <Button layoutX="41.0" layoutY="35.0" mnemonicParsing="false" text="&lt;" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="40.0">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="usernameField" layoutX="364.0" layoutY="241.0">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordField" layoutX="363.0" layoutY="316.0">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </TextField>
      <Label layoutX="368.0" layoutY="225.0" text="Enter username">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Label layoutX="366.0" layoutY="299.0" text="Enter password">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="477.0" layoutY="401.0" mnemonicParsing="false" onAction="#clickLogin" text="Login">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <Label fx:id="errorLabel" layoutX="481.0" layoutY="361.0" text="errorLabel" textFill="RED">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
